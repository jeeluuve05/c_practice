#include <stdio.h>

// Program to find the larger of two numbers

// Function returns the larger value
int findMax(int a, int b) {
    if (a > b)
        return a;
    else
        return b;
}

int main() {
    int num1, num2, result;

    // Input two numbers
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    // Determine the larger value
    result = findMax(num1, num2);

    // Output the result
    printf("The larger number is: %d\n", result);

    return 0;
}
